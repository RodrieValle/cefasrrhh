	<queryString>
		<![CDATA[SELECT
     CEFAS_LINEADEPLANILLA.`LDPCODIGO` AS CEFAS_LINEADEPLANILLA_LDPCODIGO,
     CEFAS_LINEADEPLANILLA.`EMPCODIGO` AS CEFAS_LINEADEPLANILLA_EMPCODIGO,
     CEFAS_LINEADEPLANILLA.`LDPANTICIPO` AS CEFAS_LINEADEPLANILLA_LDPANTICIPO,
     CEFAS_EMPLEADO.`EMPCODIGO` AS CEFAS_EMPLEADO_EMPCODIGO,
     CEFAS_EMPLEADO.`EMPNOMBRE` AS CEFAS_EMPLEADO_EMPNOMBRE,
     CEFAS_EMPLEADO.`EMPSALARIO` AS CEFAS_EMPLEADO_EMPSALARIO
FROM
     `CEFAS_EMPLEADO` CEFAS_EMPLEADO INNER JOIN `CEFAS_LINEADEPLANILLA` CEFAS_LINEADEPLANILLA ON CEFAS_EMPLEADO.`EMPCODIGO` = CEFAS_LINEADEPLANILLA.`EMPCODIGO` WHERE CEFAS_LINEADEPLANILLA.LDPANTICIPO != 0.00 AND CEFAS_LINEADEPLANILLA.PLNCODIGO = $P{idPlanilla}]]>
	</queryString>
